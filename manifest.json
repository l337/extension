{
	"name": "Bookmark",
	"version": "0.0.1",
	"description": "Smart One Click Bookmark",
	"permissions": ["tabs", "bookmarks"],
	"background": {
		"scripts": ["backprocess.js"],
		"persistent": false
	},
	"browser_action": {},
    "icons": {
      "16": "./images/16x16.png",
      "32": "./images/32x32.png",
      "48": "./images/48x48.png",
      "128": "./images/128x128.png"
    },
    "manifest_version": 2
}